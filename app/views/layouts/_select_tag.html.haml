#color_tag_field.col-md-4.col-md-offset-3
  .navbar-form.navbar-left.color_tag_filed
    = collection_radio_buttons :colorFilter, nil, options_for_color.push(['#ffffff', '']), :last, :first do |b|
      = b.radio_button({class: 'color-radio selected-radio', 'v-model' => 'colorFilter'})
      = label_tag "colorFilter__#{b.value}", '', style: "background-color: #{b.text}"